					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type>discreet</Type>
				<Discreets>
					<Discreet>
						<Display>Instant</Display>
						<Value>0</Value>
					</Discreet>
					<Discreet>
						<Display>5 Seconds</Display>
						<Value>5</Value>
					</Discreet>
				</Discreets>
			</Measurement>
